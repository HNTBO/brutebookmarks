{
  "name": "brute-bookmarks",
  "version": "1.0.0",
  "description": "Self-hosted bookmark dashboard with automatic icon fetching",
  "main": "server.js",
  "scripts": {
    "start": "node -r dotenv/config server.js",
    "dev": "concurrently \"vite\" \"nodemon -r dotenv/config server.js\"",
    "dev:client": "vite",
    "dev:server": "nodemon -r dotenv/config server.js",
    "build": "vite build",
    "preview": "vite preview"
  },
  "keywords": [
    "bookmarks",
    "dashboard",
    "self-hosted"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@clerk/clerk-js": "^5.122.1",
    "@clerk/express": "^1.3.0",
    "axios": "^1.6.0",
    "convex": "^1.31.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  },
  "devDependencies": {
    "@types/node": "^25.2.2",
    "concurrently": "^9.2.1",
    "nodemon": "^3.0.1"
  }
}
